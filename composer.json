{
    "name": "patronbase/omnipay-bpoint",
    "type": "library",
    "description": "BPOINT driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "bpoint",
        "commonwealth",
        "bank",
        "australia",
        "CBA",
        "redirect"
    ],
    "homepage": "https://github.com/PatronBase/omnipay-bpoint",
    "license": "MIT",
    "authors": [
        {
            "name": "Leith Caldwell",
            "email": "leith@codedruids.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\BPOINT\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\BPOINT\\" : "tests/" }
    },
    "require": {
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "prefer-stable": true
}
