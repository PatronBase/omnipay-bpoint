{
    "name": "patronbase/omnipay-bpoint",
    "type": "library",
    "description": "BPOINT driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "bpoint",
        "commonwealth",
        "bank",
        "australia",
        "CBA",
        "redirect"
    ],
    "homepage": "https://github.com/PatronBase/omnipay-bpoint",
    "license": "MIT",
    "authors": [
        {
            "name": "Leith Caldwell",
            "email": "leith@codedruids.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\BPOINT\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\BPOINT\\" : "tests/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3.1"
    },
    "require-dev": {
        "omnipay/tests": "^4.2",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/psr-http-message-bridge": "~1.1.1|^2.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2.x-dev"
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
